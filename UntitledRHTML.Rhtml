<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Update Text from URL</title>
</head>
<body>
<div id="updatedText">
    <script>
        function updateText() {
            fetch('http://127.0.0.1:9999/log/log.txt')
            .then(response => response.text())
            .then(data => {
                // Split the data by lines
                const lines = data.split('\n');
                // Get the last 10 lines
                const lastTenLines = lines.slice(-10).join('\n');
                // Update the text in the HTML element
                document.getElementById('updatedText').textContent = lastTenLines;
            });
        }

        // Update text every 5 seconds
        setInterval(updateText, 5000);

        // Call updateText initially to update the text when the page loads
        updateText();
    </script>
</div>
</body>
</html>
